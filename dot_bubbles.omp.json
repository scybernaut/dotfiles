{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#241F31",
          "foreground": "#62A0EA",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "full",
            "template": "\ue5ff {{ .Path }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "foreground": "#33D17A",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "branch_icon": "",
            "template": " {{ .HEAD }}{{ .BranchStatus }}{{ if or (.Working.Changed) (.Staging.Changed) }} {{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}  \uF692 {{ .StashCount }}{{ end }}"
          },
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#241F31",
          "foreground": "#F66151",
          "leading_diamond": "\ue0b6",
          "properties": {
            "template": "{{ if gt .Code 0 }}\uf00d {{ if eq .Meaning (toString .Code) -}} {{.Code}} {{- else -}} {{.Meaning}} ({{.Code}}) {{- end }}{{ end }}"
          },
          "trailing_diamond": "\ue0b4 ",
          "style": "diamond",
          "type": "exit"
        },
        {
          "background": "#241F31",
          "foreground": "#9A9996",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "austin",
            "template": "{{ .FormattedMs }}",
            "threshold": 150
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4 ",
          "type": "executiontime"
        },
        {
          "background": "#241F31",
          "foreground": "#F6D32D",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#241F31",
          "foreground": "#62A0EA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "go"
        },
        {
          "background": "#241F31",
          "foreground": "#2EC27E",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#241F31",
          "foreground": "#E01B24",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "ruby"
        },
        {
          "background": "#241F31",
          "foreground": "#FF7800",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "java"
        },
        {
          "background": "#241F31",
          "foreground": "#C061CB",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "julia"
        },
        {
          "background": "#241F31",
          "foreground": "#DC8ADD",
          "leading_diamond": "\ue0b6",
          "properties": {
            "template": "{{ if .SSHSession }}\uF817 {{ end }}{{ .UserName }}@{{ .HostName }}"
          },
          "style": "diamond",
	  "trailing_diamond": "\ue0b4",
          "type": "session"
        },
        {
          "background": "#241F31",
          "foreground": "#9B6BDF",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "leading_diamond": " \ue0b6",
          "properties": {
            "charged_icon": " ",
            "charging_icon": "\u21e1 ",
            "discharging_icon": "\u21e3 ",
            "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "battery"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#F9F06B",
          "properties": {
            "template": "✌️ "
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 1
}
